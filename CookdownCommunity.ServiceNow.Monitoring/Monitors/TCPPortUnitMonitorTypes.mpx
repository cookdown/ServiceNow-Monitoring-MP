              </Node>
            </RegularDetection>
            <RegularDetection MonitorTypeStateID="Unacceptable">
              <Node ID="CDUnacceptable">
                <Node ID="DS1" />
              </Node>
            </RegularDetection>
          </RegularDetections>
        </MonitorImplementation>
